    "@radix-ui/react-slot": "^1.1.2",
    "@turf/turf": "^7.2.0",
    "ai": "^4.1.54",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.13",
    "lodash": "^4.17.21",
    "lucide-react": "^0.479.0",
    "maplibre-gl": "^5.2.0",
    "next": "15.2.2",
