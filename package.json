    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --fix .",
    "check:updates": "pnpm dlx npm-check-updates --interactive --format group",
    "check:knip": "knip",
    "parse-manifestos": "tsx ./src/scripts/parse-manifestos.ts",
    "validate-data": "tsx ./src/scripts/validate-data.ts",
    "prepare": "husky install",
    "db:generate": "supabase gen types typescript --local > src/models/database.ts",
    "db:migrate:up": "supabase migration up && pnpm db:generate",
    "db:reset": "supabase db reset"
