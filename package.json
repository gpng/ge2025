    "knip": "^5.46.2",
    "lint-staged": "^15.5.0",
    "supabase": "^2.20.12",
    "tailwind-scrollbar": "^4.0.1",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "sharp", "supabase"]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["pnpm lint:fix"],
    "data/*.json": ["pnpm validate-data", "pnpm lint:fix"]
  }
