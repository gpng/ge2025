    "next-themes": "^0.4.6",
    "openai": "^4.89.1",
    "pdf-parse": "^1.1.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-map-gl": "^8.0.1",
    "react-markdown": "^10.1.0",
